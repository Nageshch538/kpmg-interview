{
  "family": "flaskapp-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "flaskapp",
      "image": "IMAGE_URI"
,
      "portMappings": [
        {
          "containerPort": 5000
        }
      ],
      "essential": true
    }
  ]
}
